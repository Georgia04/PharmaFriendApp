<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GPSLocationActivity">

    <fragment
        android:id="@+id/google_map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="match_parent"
        android:layout_height="413dp"
        tools:layout="@layout/activity_fragment_location" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="320dp"
        android:layout_height="25dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="18dp"
        android:layout_marginEnd="73dp"
        android:layout_marginBottom="194dp"
        android:text="Is this your correct location?"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textViewPositive"
        android:layout_width="298dp"
        android:layout_height="36dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="19dp"
        android:layout_marginEnd="94dp"
        android:layout_marginBottom="100dp"
        android:text="Yes, this is my location"
        android:textColor="#000000"
        android:textSize="19sp" />

    <TextView
        android:id="@+id/textViewNegative"
        android:layout_width="309dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="14dp"
        android:layout_marginEnd="88dp"
        android:layout_marginBottom="152dp"
        android:text="No,  this is not my location"
        android:textColor="#000000"
        android:textSize="19sp" />

</RelativeLayout>